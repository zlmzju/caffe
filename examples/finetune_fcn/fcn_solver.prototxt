# The train/test net protocol buffer definition
net: "fcn_finetune_train.prototxt"
test_iter: 100
test_interval: 1000
base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 100
display: 10
max_iter: 5000
momentum: 0.9
weight_decay: 0.0005
snapshot: 1000
snapshot_prefix: "fcn"
# solver mode: CPU or GPU
solver_mode: GPU

